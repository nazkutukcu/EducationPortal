@using EducationPortal.Entities.DTOs.StudentDtos;
@model List<StudentDto>

@{
    ViewData["Title"] = "All Students";
}

<h1>All Users</h1>

<table class="table">
    <thead>
        <tr>
            <th>FirstName</th>
            <th>LastName</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var student in Model)
        {
            <tr>
                <td>@student.FirstName</td>
                <td>@student.LastName</td>
                <td>@student.Email</td>
            </tr>
        }
    </tbody>
</table>
